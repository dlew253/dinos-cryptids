<h2>Heres all the dinos</h2>
<ul>
    <% dinos.forEach((dino, index) => { %>
        <li>
            <a href="/dinos/<%= index %>"><%= dino.name %></a>
            <form method="POST" action="/dinos/<%= index %>/?_method=DELETE">
                <input type="submit" value="Delete">
            </form>
        </li>
    <% }) %>

</ul>